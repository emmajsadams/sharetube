{
  "name": "blocktube",
  "author": "Cody Stebbins",
  "version": "1.0.0",
  "description": "staytuned",
  "keywords": [
    "tube"
  ],
  "license": "MIT",
  "homepage": "https://github.com/codystebbins/blocktube#readme",
  "main": "src/index.js",
  "bugs": {
    "url": "https://github.com/codystebbins/blocktube/issues"
  },
  "engines": {
    "node": "5.10.0"
  },
  "scripts": {
    "build": "npm run lint && npm run bundle-app -- -t uglifyify -o tmp/bundle.js && uglifyjs tmp/bundle.js -c > app/bundle.js",
    "start": "npm run bundle-app -- -w -o app/bundle.js & npm run watch-test & npm run static & livereloadify app",
    "browser-test": "npm run bundle-test && testem ci -P 5 | tap-min",
    "test": "npm run lint && npm run tape --silent | tap-min",
    "static": "static app",
    "lint": "eslint src/**",
    "tape": "tape -r babel-register src/**/*.test.js",
    "watch-test": "watch 'npm run tape --silent | tap-notify | tap-min' src/",
    "bundle-app": "globify ./src/index.js -t babelify -p errorify",
    "bundle-test": "globify 'src/**/*.test.js' -o 'tmp/test-bundle.js' -i 'react/lib/ReactContext' -i 'react/lib/ExecutionEnvironment' -t babelify -p errorify"
  },
  "repository": {
    "type": "git",
    "url": "git+https://codystebbins@github.com/codystebbins/blocktube.git"
  },
  "dependencies": {
    "fjs": "^0.5.0",
    "immutable": "^3.7.6",
    "isomorphic-fetch": "^2.2.1",
    "lodash": "^4.6.1",
    "react": "^0.14.7",
    "react-addons-css-transition-group": "^0.14.7",
    "react-bootstrap": "^0.28.3",
    "react-disqus-thread": "^0.3.1",
    "react-dom": "^0.14.7",
    "react-notifications": "^1.0.1",
    "react-redux": "^4.4.1",
    "react-router": "^2.0.1",
    "react-youtube": "^5.1.0",
    "redux": "^3.3.1",
    "redux-actions": "^0.9.1",
    "redux-immutable": "^3.0.6",
    "redux-saga": "^0.9.5"
  },
  "devDependencies": {
    "babel-core": "^6.7.2",
    "babel-eslint": "^5.0.0",
    "babel-polyfill": "^6.7.4",
    "babel-preset-es2015": "^6.6.0",
    "babel-preset-react": "^6.5.0",
    "babel-register": "^6.7.2",
    "babelify": "^7.2.0",
    "browserify": "^13.0.0",
    "enzyme": "^2.2.0",
    "errorify": "^0.3.1",
    "eslint": "^2.5.1",
    "eslint-config-airbnb": "^6.1.0",
    "eslint-loader": "^1.3.0",
    "eslint-plugin-react": "^4.2.3",
    "fetch-mock": "^4.3.0",
    "globify": "^1.2.1",
    "livereloadify": "^2.0.0",
    "node-static": "^0.7.7",
    "phantomjs-prebuilt": "^2.1.7",
    "react-addons-test-utils": "^0.14.7",
    "sinon": "^1.17.3",
    "sinon-as-promised": "^4.0.0",
    "tap-min": "^1.1.0",
    "tap-notify": "^1.0.0",
    "tape": "^4.5.1",
    "teaspoon": "^6.3.0",
    "testem": "^1.6.0",
    "uglify-js": "^2.6.2",
    "uglifyify": "^3.0.1",
    "watch": "^0.17.1"
  }
}
