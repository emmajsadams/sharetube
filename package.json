{
  "name": "blocktube",
  "version": "1.0.0",
  "description": "staytuned",
  "main": "index.js",
  "scripts": {
    "build": "eslint src/** && browserify -t babelify ./src/index.js | uglifyjs -c > app/bundle.js",
    "watch-test": "globify 'src/**/*.test.js' -w -o 'tmp/test-bundle.js' -i 'react/lib/ReactContext' -i 'react/lib/ExecutionEnvironment' -t babelify -p errorify -p proxyquire-universal",
    "start": "npm run watch-app & static app & npm run watch-test",
    "test": "testem",
    "test-browsers": "testem ci -P 30",
    "watch-app": "watchify -t babelify -p errorify ./src/index.js -o app/bundle.js & livereloadify app"
  },
  "repository": {
    "type": "git",
    "url": "git+https://codystebbins@github.com/codystebbins/blocktube.git"
  },
  "keywords": [
    "tube",
    "./src/*/*.js"
  ],
  "author": "Cody Stebbins",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/codystebbins/blocktube/issues"
  },
  "homepage": "https://github.com/codystebbins/blocktube#readme",
  "dependencies": {
    "fjs": "^0.5.0",
    "guid": "0.0.12",
    "lodash": "^4.6.1",
    "react": "^0.14.7",
    "react-addons-css-transition-group": "^0.14.7",
    "react-bootstrap": "^0.28.3",
    "react-disqus-thread": "^0.3.1",
    "react-dom": "^0.14.7",
    "react-notifications": "^1.0.1",
    "react-redux": "^4.4.1",
    "react-router": "^2.0.1",
    "react-youtube": "^5.1.0",
    "redux": "^3.3.1",
    "redux-actions": "^0.9.1",
    "redux-saga": "^0.9.5"
  },
  "devDependencies": {
    "babel-core": "^6.7.2",
    "babel-eslint": "^5.0.0",
    "babel-loader": "^6.2.4",
    "babel-preset-es2015": "^6.6.0",
    "babel-preset-react": "^6.5.0",
    "babelify": "^7.2.0",
    "blue-tape": "^0.2.0",
    "browserify": "^13.0.0",
    "enzyme": "^2.2.0",
    "errorify": "^0.3.1",
    "eslint": "^2.5.1",
    "eslint-config-airbnb": "^6.1.0",
    "eslint-loader": "^1.3.0",
    "eslint-plugin-react": "^4.2.3",
    "fetch-mock": "^4.3.0",
    "glob": "^7.0.3",
    "globify": "^1.2.1",
    "livereloadify": "^2.0.0",
    "node-static": "^0.7.7",
    "phantomjs-prebuilt": "^2.1.7",
    "proxyquire": "^1.7.4",
    "proxyquire-universal": "^1.0.8",
    "proxyquireify": "^3.1.1",
    "react-addons-test-utils": "^0.14.7",
    "react-dom": "^0.14.7",
    "redux-mock-store": "^1.0.2",
    "sinon": "^1.17.3",
    "sinon-as-promised": "^4.0.0",
    "testem": "^1.6.0",
    "uglify-js": "^2.6.2",
    "uglifyify": "^3.0.1"
  }
}
